{
  "$schema": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/CustomerShippingAddress/v1/1_002/jsonschema/schemas/CustomerShippingAddress_1_002.json#",
  "info": {
    "description": "Api para a entidade Local de Entrega (CustomerShippingAddress) para produtos TOTVS",
    "version": "1.002",
    "title": "CustomerShippingAddress",
    "contact": {
      "name": "T-Talk",
      "url": "api.totvs.com.br",
      "email": "comiteintegracao@totvs.com.br"
    },
    "x-totvs": {
      "transactionDefinition": {
        "subType": "event",
        "businessContentType": {
          "$ref": "#/definitions/CustomerShippingAddressInfo",
          "type": "object"
        },
        "returnContentType": {
          "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/ListOfInternalId_1_000.json#/definitions/ReturnContentWithModelType",
          "type": "object"
        }
      },
      "messageDocumentation": {
        "name": "CustomerShippingAddress",
        "description": "Local de Entrega",
        "segment": "Distribuição"
      },
      "productInformation": [
        {
          "product": "Datasul",
          "contact": "ieda.soares@totvs.com.br",
          "description": "Cadastro de Locais de Entrega ",
          "adapter": "CustomerShippingAddress.p"
        }
      ]
    }
  },
  "definitions": {
    "PagedCustomerShippingAddress": {
      "type": "object",
      "allOf": [
        {
          "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
        },
        {
          "$ref": "#/definitions/CustomerShippingAddresses"
        }
      ]
    },
    "CustomerShippingAddresses": {
      "type": "object",
      "properties": {
        "items":{
          "type": "array",
          "items": {
            "$ref": "#/definitions/CustomerShippingAddressInfo"
          }
        }
      }
    },
    "CustomerShippingAddressInfo": {
      "description": "Descreve um local de entrega relacionado a um emitente",
      "type": "object",
      "properties": {
        "CustomerCode": {
          "description": "Código do cliente/fornecedor",
          "type": "string",
          "maxLength": 20,
          "x-totvs": [
            {
              "product": "Datasul",
              "field": "loc-entr.cod-emitente",
              "required": true,
              "type": "integer",
              "length": "9",
              "note": "Código do Emitente",
              "available": true,
              "canUpdate": false
            }
          ]
        },
        "CustomerInternalId": {
          "description": "InternalID do cliente/fornecedor",
          "type": "string",
          "maxLength": 20,
          "x-totvs": [
            {
              "product": "Datasul",
              "field": "loc-entr.cod-emitente",
              "required": true,
              "type": "integer",
              "length": "9",
              "note": "O InternalID do Emitente no Datasul é formado apenas pelo próprio código do cliente ou fornecedor",
              "available": true,
              "canUpdate": false
            }
          ]
        },
        "GovernmentalInformation": {
          "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/types/GovernmentalInformation_1_000.json#/definitions/GovernmentalInformationType",
          "type": "object"
        },
        "InternalId": {
          "description": "InternalId da entidade",
          "type": "string",
          "x-totvs": [
            {
              "product": "Datasul",
              "field": "emitente|entrega",
              "required": "true",
              "type": "string",
              "length": "22",
              "note": "O InternalId do endereço de entrega no Datasul é formado pela junção dos campos loc-entr.cod-emitente|loc-entr.cod-entrega",
              "available": true,
              "canUpdate": false
            }
          ]
        },
        "Code": {
          "description": "Código do Local de Entrega",
          "type": "string",
          "x-totvs": [
            {
              "product": "DATASUL",
              "field": "loc-entr.cod-entrega",
              "required": false,
              "type": "string",
              "length": "12",
              "note": "Código do Local de Entrega Formato do Campo: x(12)",
              "available": true,
              "canUpdate": false
            },
            {
              "product": "LOGIX",
              "field": "",
              "required": false,
              "type": "",
              "length": "",
              "note": "",
              "available": true,
              "canUpdate": false
            },
            {
              "product": "PROTHEUS",
              "field": "",
              "required": false,
              "type": "",
              "length": "",
              "note": "",
              "available": true,
              "canUpdate": false
            },
            {
              "product": "RM",
              "field": "",
              "required": false,
              "type": "",
              "length": "",
              "note": "",
              "available": true,
              "canUpdate": false
            }
          ]
        },
        "ExternalId": {
          "description": "Id Externo do local de entrega",
          "type": "string",
          "maxLength": "9",
          "x-totvs": [
            {
              "product": "DATASUL",
              "field": "loc-entr.id-externo",
              "required": true,
              "type": "integer",
              "length": "9",
              "note": "Código sequencial do local de entrega  Formato do Campo: >>>>>>>>9 O campo loc-entr.cod-entrega receberá o conteúdo TMS ou WMS mais o valor desta tag. Exemplo: TMS 1",
              "available": true,
              "canUpdate": false
            }
          ]
        },
        "ShippingAddressInformation": {
          "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/types/GovernmentalInformation_1_000.json#/definitions/GovernmentalInformationType",
          "description": "Informações do endereço de entrega - CNPJ - Inscrição Estadual",
          "type": "object"
        },
        "ShippingAddress": {
          "description": "Informações do endereço de entrega",
          "type": "array",
          "items": {
            "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/types/Address_1_000.json#/definitions/AddressType",
            "type": "object"
          },
          "x-totvs": [
            {
              "product": "Datasul",
              "field": ": Address = loc-entr.endereco(40), Number = ???, Complement = loc-entr.endereco_text, CityCode = ???, CityName = loc-entr.cidade(25), District = loc-entr.bairro(30), State = loc-entr.estado(4), CountryCode = loc-entr.pais, ZIPCode = loc-entr.cep(12), Region=???, POBox=loc-entr.caixa-postal",
              "required": true,
              "note": "Neste objeto estão as informações do endereço de entrega conforme os campos especificados",
              "available": true,
              "canUpdate": true
            }
          ]
        },
        "CarrierCode": {
          "description": "Código da transportadora informada no local de entrega do cliente",
          "type": "string",
          "maxLength": "12",
          "x-totvs": [
            {
              "product": "PROTHEUS",
              "field": "",
              "required": false,
              "type": "",
              "length": "",
              "note": "",
              "available": true,
              "canUpdate": false
            },
            {
              "product": "RM",
              "field": "",
              "required": false,
              "type": "",
              "length": "",
              "note": "",
              "available": true,
              "canUpdate": false
            },
            {
              "product": "DATASUL",
              "field": "cod-transp",
              "required": true,
              "type": "integer",
              "length": "5",
              "note": "Código do Transportador",
              "available": true,
              "canUpdate": false
            },
            {
              "product": "LOGIX",
              "field": "",
              "required": false,
              "type": "",
              "length": "",
              "note": "",
              "available": true,
              "canUpdate": false
            }
          ]
        },
        "CarrierInternalId": {
          "descrption": "InternalId da transportadora informada no local de entrega do cliente",
          "type": "string",
          "x-totvs": [
            {
              "product": "DATASUL",
              "field": "cod-transp",
              "required": true,
              "type": "integer",
              "length": "5",
              "note": "O internalId no Datasul é formado somente pelo Código do Transportador",
              "available": true,
              "canUpdate": false
            }
          ]
        }
      }
    }
  }
}